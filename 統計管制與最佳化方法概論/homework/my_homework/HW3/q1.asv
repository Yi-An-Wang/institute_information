pdf=zeros(3,999);
b=[5 8 13];
X=zeros(3,999);
for ii=1:3
    x=linspace(0,b(ii),1000);
    P=q1_pdf(x,b(ii));% parameter=b(ii)
    P(1)=[];
    x(1)=[];
    X(ii,:)=x;
    pdf(ii,:)=P;
end
for ii=1:3
    plot(X(ii,:),pdf(ii,:))
    grid on
    hold on
end
xlabel()
legend('b=5','b=8','b=13');
hold off